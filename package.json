{
  "devDependencies": {
    "esbuild": "^0.14.39",
    "tailwindcss": "^3.0.24"
  },
  "scripts": {
    "build:html": "bundle exec jekyll build",
    "watch:html": "bundle exec jekyll serve --livereload",
    "build:css": "npx tailwindcss -i ./src/assets/stylesheets/tailwind.css -o ./dist/assets/stylesheets/main.css --minify",
    "watch:css": "npx tailwindcss -i ./src/assets/stylesheets/tailwind.css -o ./dist/assets/stylesheets/main.css --minify --watch",
    "build:js": "./node_modules/.bin/esbuild ./src/assets/javascripts/app.js --bundle --sourcemap --outfile=./dist/assets/javascripts/main.js",
    "watch:js": "./node_modules/.bin/esbuild ./src/assets/javascripts/app.js --bundle --sourcemap --outfile=./dist/assets/javascripts/main.js --watch"
  },
  "dependencies": {}
}
