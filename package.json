{
  "devDependencies": {
    "tailwindcss": "^3.0.24"
  },
  "scripts": {
    "build:html": "bundle exec jekyll build",
    "watch:html": "bundle exec jekyll serve --livereload",
    "build:css": "npx tailwindcss -i tailwind.css -o ./dist/main.css --minify",
    "watch:css": "npx tailwindcss -i tailwind.css -o ./dist/main.css --minify --watch"
  },
  "dependencies": {
  }
}
